{
  "tags":    {
    "allowUnknownTags": true
  },
  "source":  {
    "includePattern": ".+\\.(js(doc|x)?|mjs)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts":    {
    "template":    "node_modules/docdash",
    "encoding":    "utf8",
    "destination": "docs/",
    "recurse":     true
  },
  "plugins": [
    "node_modules/jsdoc-export-default-interop/dist/index"
  ]
}
